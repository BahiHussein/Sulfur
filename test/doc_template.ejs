<!doctype html>
<html lang="en" data-bs-theme="auto">

<head>
    <script src="../assets/js/color-modes.js"></script>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="Mark Otto, Jacob Thornton, and Bootstrap contributors">
    <meta name="generator" content="Hugo 0.111.3">
    <title> <%=title%></title>

    <link rel="canonical" href="https://getbootstrap.com/docs/5.3/examples/heroes/">


    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet">

    <style>
        .bd-placeholder-img {
            font-size: 1.125rem;
            text-anchor: middle;
            -webkit-user-select: none;
            -moz-user-select: none;
            user-select: none;
        }

        @media (min-width: 768px) {
            .bd-placeholder-img-lg {
                font-size: 3.5rem;
            }
        }

        .b-example-divider {
            width: 100%;
            height: 3rem;
            background-color: rgba(0, 0, 0, .1);
            border: solid rgba(0, 0, 0, .15);
            border-width: 1px 0;
            box-shadow: inset 0 .5em 1.5em rgba(0, 0, 0, .1), inset 0 .125em .5em rgba(0, 0, 0, .15);
        }

        .b-example-vr {
            flex-shrink: 0;
            width: 1.5rem;
            height: 100vh;
        }

        .bi {
            vertical-align: -.125em;
            fill: currentColor;
        }

        .nav-scroller {
            position: relative;
            z-index: 2;
            height: 2.75rem;
            overflow-y: hidden;
        }

        .nav-scroller .nav {
            display: flex;
            flex-wrap: nowrap;
            padding-bottom: 1rem;
            margin-top: -1px;
            overflow-x: auto;
            text-align: center;
            white-space: nowrap;
            -webkit-overflow-scrolling: touch;
        }

        .btn-bd-primary {
            --bd-violet-bg: #712cf9;
            --bd-violet-rgb: 112.520718, 44.062154, 249.437846;

            --bs-btn-font-weight: 600;
            --bs-btn-color: var(--bs-white);
            --bs-btn-bg: var(--bd-violet-bg);
            --bs-btn-border-color: var(--bd-violet-bg);
            --bs-btn-hover-color: var(--bs-white);
            --bs-btn-hover-bg: #6528e0;
            --bs-btn-hover-border-color: #6528e0;
            --bs-btn-focus-shadow-rgb: var(--bd-violet-rgb);
            --bs-btn-active-color: var(--bs-btn-hover-color);
            --bs-btn-active-bg: #5a23c8;
            --bs-btn-active-border-color: #5a23c8;
        }

        .bd-mode-toggle {
            z-index: 1500;
        }





        .dropdown-toggle {
            outline: 0;
        }

        .btn-toggle {
            padding: .25rem .5rem;
            font-weight: 600;
            color: var(--bs-emphasis-color);
            background-color: transparent;
        }

        .btn-toggle:hover,
        .btn-toggle:focus {
            color: rgba(var(--bs-emphasis-color-rgb), .85);
            background-color: var(--bs-tertiary-bg);
        }

        .btn-toggle::before {
            width: 1.25em;
            line-height: 0;
            content: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' width='16' height='16' viewBox='0 0 16 16'%3e%3cpath fill='none' stroke='rgba%280,0,0,.5%29' stroke-linecap='round' stroke-linejoin='round' stroke-width='2' d='M5 14l6-6-6-6'/%3e%3c/svg%3e");
            transition: transform .35s ease;
            transform-origin: .5em 50%;
        }

        [data-bs-theme="dark"] .btn-toggle::before {
            content: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' width='16' height='16' viewBox='0 0 16 16'%3e%3cpath fill='none' stroke='rgba%28255,255,255,.5%29' stroke-linecap='round' stroke-linejoin='round' stroke-width='2' d='M5 14l6-6-6-6'/%3e%3c/svg%3e");
        }

        .btn-toggle[aria-expanded="true"] {
            color: rgba(var(--bs-emphasis-color-rgb), .85);
        }

        .btn-toggle[aria-expanded="true"]::before {
            transform: rotate(90deg);
        }

        .btn-toggle-nav a {
            padding: .1875rem .5rem;
            margin-top: .125rem;
            margin-left: 1.25rem;
        }

        .btn-toggle-nav a:hover,
        .btn-toggle-nav a:focus {
            background-color: var(--bs-tertiary-bg);
        }

        .scrollarea {
            overflow-y: auto;
        }
    </style>



</head>

<body>



    <div class="flex-shrink-0 p-3" style="width: 280px; position:fixed;">
        <a href="#" class="d-flex align-items-center pb-3 mb-3 link-body-emphasis text-decoration-none border-bottom">

            <span class="fs-5 fw-semibold">Scenarios</span>
        </a>
        <ul class="list-unstyled ps-0">
            <% for (var i=0; i < scenarios.length; i++) { %>
                <li class="mb-1">
                    <button class="btn btn-toggle d-inline-flex align-items-center rounded border-0 collapsed"
                        data-bs-toggle="collapse" data-bs-target="#<%= scenarios[i].id %>-collapse"
                        aria-expanded="true">
                        <%= scenarios[i].title %>
                    </button>
                    <div class="collapse show" id="<%= scenarios[i].id %>-collapse">

                        <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                            <% for (var x=0; x < scenarios[i].steps.steps.length; x++) { %>
                                <li><a href="#<%=scenarios[i].steps.steps[x].id%>"
                                        class="link-body-emphasis d-inline-flex text-decoration-none rounded">
                                        <%=scenarios[i].steps.steps[x].title%>
                                    </a></li>
                                <% } %>
                        </ul>
                    </div>
                </li>
                <% } %>


        </ul>
    </div>






    <main>



        <div class="px-4 py-5 my-5 text-center">
            <div>
                <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="160"
                    zoomAndPan="magnify" viewBox="0 0 120 119.999999" height="160" preserveAspectRatio="xMidYMid meet"
                    version="1.0">
                    <path fill="#ee4d28"
                        d="M 109.453125 0 L 52.96875 0 C 47.152344 0 42.421875 4.730469 42.421875 10.546875 L 42.421875 109.453125 C 42.421875 115.269531 47.152344 120 52.96875 120 L 109.453125 120 C 115.269531 120 120 115.269531 120 109.453125 L 120 10.546875 C 120 4.730469 115.269531 0 109.453125 0 Z M 112.96875 109.453125 C 112.96875 111.390625 111.390625 112.96875 109.453125 112.96875 L 52.96875 112.96875 C 51.03125 112.96875 49.453125 111.390625 49.453125 109.453125 L 49.453125 105.9375 L 112.96875 105.9375 Z M 112.96875 98.90625 L 49.453125 98.90625 L 49.453125 56.25 L 112.96875 56.25 Z M 56.484375 24.609375 C 56.484375 22.671875 58.0625 21.09375 60 21.09375 C 61.9375 21.09375 63.515625 22.671875 63.515625 24.609375 C 63.515625 26.546875 61.9375 28.125 60 28.125 C 58.0625 28.125 56.484375 26.546875 56.484375 24.609375 Z M 77.578125 24.609375 C 77.578125 22.671875 79.15625 21.09375 81.09375 21.09375 C 83.03125 21.09375 84.609375 22.671875 84.609375 24.609375 C 84.609375 26.546875 83.03125 28.125 81.09375 28.125 C 79.15625 28.125 77.578125 26.546875 77.578125 24.609375 Z M 98.90625 24.609375 C 98.90625 22.671875 100.484375 21.09375 102.421875 21.09375 C 104.359375 21.09375 105.9375 22.671875 105.9375 24.609375 C 105.9375 26.546875 104.359375 28.125 102.421875 28.125 C 100.484375 28.125 98.90625 26.546875 98.90625 24.609375 Z M 112.96875 24.609375 L 112.96875 49.21875 L 105.9375 49.21875 L 105.9375 34.550781 C 110.03125 33.101562 112.96875 29.191406 112.96875 24.609375 C 112.96875 18.792969 108.238281 14.0625 102.421875 14.0625 C 96.605469 14.0625 91.875 18.792969 91.875 24.609375 C 91.875 29.191406 94.8125 33.101562 98.90625 34.550781 L 98.90625 49.21875 L 84.609375 49.21875 L 84.609375 34.550781 C 88.703125 33.101562 91.640625 29.191406 91.640625 24.609375 C 91.640625 18.792969 86.910156 14.0625 81.09375 14.0625 C 75.277344 14.0625 70.546875 18.792969 70.546875 24.609375 C 70.546875 29.191406 73.488281 33.101562 77.578125 34.550781 L 77.578125 49.21875 L 63.515625 49.21875 L 63.515625 34.550781 C 67.609375 33.101562 70.546875 29.191406 70.546875 24.609375 C 70.546875 18.792969 65.816406 14.0625 60 14.0625 C 54.183594 14.0625 49.453125 18.792969 49.453125 24.609375 C 49.453125 29.191406 52.394531 33.101562 56.484375 34.550781 L 56.484375 49.21875 L 49.453125 49.21875 L 49.453125 10.546875 C 49.453125 8.609375 51.03125 7.03125 52.96875 7.03125 L 109.453125 7.03125 C 111.390625 7.03125 112.96875 8.609375 112.96875 10.546875 L 112.96875 24.609375 "
                        fill-opacity="1" fill-rule="nonzero" />
                    <path fill="#ee4d28"
                        d="M 32.808594 22.859375 L 20.621094 1.757812 C 19.996094 0.667969 18.835938 0 17.578125 0 C 16.320312 0 15.160156 0.667969 14.535156 1.757812 L 2.347656 22.859375 C 0.8125 25.519531 0 28.554688 0 31.640625 C 0 40.128906 6.046875 47.230469 14.0625 48.867188 L 14.0625 116.484375 C 14.0625 118.425781 15.636719 120 17.578125 120 C 19.519531 120 21.09375 118.425781 21.09375 116.484375 L 21.09375 48.867188 C 29.109375 47.230469 35.15625 40.128906 35.15625 31.640625 C 35.15625 28.554688 34.34375 25.519531 32.808594 22.859375 Z M 17.578125 42.1875 C 15.640625 42.1875 14.0625 40.609375 14.0625 38.671875 C 14.0625 36.734375 15.640625 35.15625 17.578125 35.15625 C 19.515625 35.15625 21.09375 36.734375 21.09375 38.671875 C 21.09375 40.609375 19.515625 42.1875 17.578125 42.1875 Z M 27.519531 35.15625 C 26.070312 31.0625 22.160156 28.125 17.578125 28.125 C 12.996094 28.125 9.085938 31.0625 7.636719 35.15625 C 7.246094 34.054688 7.03125 32.871094 7.03125 31.640625 C 7.03125 29.789062 7.515625 27.96875 8.4375 26.375 L 17.578125 10.546875 L 26.71875 26.375 C 27.640625 27.96875 28.125 29.789062 28.125 31.640625 C 28.125 32.875 27.910156 34.054688 27.519531 35.15625 "
                        fill-opacity="1" fill-rule="nonzero" />
                </svg>
            </div>
            <div>
                <strong> SULFUR V.0.0.1</strong>
            </div>

            <br />

            <h1 class="visually-hidden">
                <%= title %>
            </h1>

            <h1 class="display-5 fw-bold text-body-emphasis">
                <%=title%>
            </h1>

            <h3 class=" fw-bold text-body-emphasis">
                <%=version%>
            </h3>
            <div class="col-lg-6 mx-auto">

                <p class="lead mb-4">
                    <%= desc %>
                </p>

            </div>
        </div>


        <% for (var i=0; i < scenarios.length; i++) { %>



            <div class="col-lg-6 mx-auto ">
                <div class="px-4 pt-5 my-5 border-bottom">
                    <h1>
                        <%= scenarios[i].title %>
                            <% if(!scenarios[i].passed){ %> <span class="badge bg-danger">Failed</span>
                                <%} else {%> <span class="badge bg-success">Passed</span>
                                    <% } %>
                    </h1>
                    <p class="lead mb-4">
                        <%= scenarios[i].desc %>
                    </p>
                    <p class="fw-bold text-body-emphasis">id: <%= scenarios[i].id %>
                    </p>
                    <% for (var x=0; x < scenarios[i].steps.steps.length; x++) { %>

                        <div class="card mb-4" id="<%=scenarios[i].steps.steps[x].id%>">
                            <div class="card-header">

                                <div class="d-flex justify-content-between">
                                    Step #<%=x+1%>

                                        <div class="d-flex">
                                            <a href="#" class="btn btn-primary">Run</a>
                                        </div>
                                </div>





                            </div>
                            <div class="card-body">
                                <h5>
                                    <%= scenarios[i].steps.steps[x].title %>
                                        <% if(!scenarios[i].steps.steps[x].passed){ %> <span
                                                class="badge bg-danger">Failed</span>
                                            <%} else {%> <span class="badge bg-success">Passed</span>
                                                <% } %>
                                </h5>

                            </div>


                            <div class="table-responsive">
                            <table class="table" >

                                <tbody>
                                    <% for(var k=0; k<plTypes.length; k++){ %>
                                        <% if(scenarios[i].steps.steps[x].runner){ %>
                                        <% if(scenarios[i].steps.steps[x].runner.payload[plTypes[k]] &&
                                            Object.keys(scenarios[i].steps.steps[x].runner.payload[plTypes[k]]).length>
                                            0){ %>
                                            <tr>
                                                <th scope="row">
                                                    <%= plTypes[k] %>
                                                </th>
                                                <td>
                                                    <% if(typeof
                                                        scenarios[i].steps.steps[x].runner.payload[plTypes[k]]=="object"
                                                        ){ %>

                                                        <pre class="px-2 py-2"
                                                            style="background-color: black; color: aliceblue; border-radius: 8px; overflow-x: auto; max-width: 600px;"><%- JSON.stringify(scenarios[i].steps.steps[x].runner.payload[plTypes[k]], undefined, 2) %></pre>


                                                        <% } else { %>
                                                            <strong><%-
                                                                    scenarios[i].steps.steps[x].runner.payload[plTypes[k]]
                                                                    %></strong>
                                                            <% } %>
                                                                <% if(scenarios[i].steps.steps[x].runner.docs?.[plTypes[k]])
                                                                    { %>
                                                                        <div class="table-responsive">
                                                                    <table class="table table-bordered border-primary"
                                                                        style="max-width: 600px">

                                                                        <tbody>

                                                                            <% Object.keys(scenarios[i].steps.steps[x].runner.docs[plTypes[k]]).forEach(function(prop)
                                                                                { %>

                                                                                <tr>
                                                                                    <th scope="row">
                                                                                        <%= prop %>
                                                                                    </th>
                                                                                    <td><%- scenarios[i].steps.steps[x].runner.docs[plTypes[k]][prop]
                                                                                            %> </td>
                                                                                </tr>
                                                                                <% }); %>



                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                                    <% } %>

                                                </td>
                                            </tr>
                                            <% } %>
                                            <% } else { %>
                                                <tr>
                                                    
                                                    <td>
                                                        <span class="badge bg-secondary">PAUSED</span>
                                                    </td>
                                                </tr>
                                                <% } %>
                                    <% } %>

                                </tbody>
                            </table>
                        </div>




                            <div class="list-group px-4 py-4">
                                <h5> Test Results </h5>
                                <% if (scenarios[i].steps.steps[x].runner) { %>
                                <% for( var r=0; r < scenarios[i].steps.steps[x].runner.rounds.length; r++ ) { %>

                                    <a href="#<%=scenarios[i].steps.steps[x].runner.rounds[r].id %>"
                                        data-bs-toggle="offcanvas" role="button"
                                        aria-controls="<%=scenarios[i].steps.steps[x].runner.rounds[r].id %>"
                                        class="list-group-item list-group-item-action" aria-current="true">
                                        <%if(scenarios[i].steps.steps[x].runner.rounds[r].res) { %>
                                            Test Passed:<%if(scenarios[i].steps.steps[x].runner.rounds[r].passed) {%>
                                                
                                                ✅<% }else{%>❌
                                                    <%}%> | SHOULD <%
                                                            if(scenarios[i].steps.steps[x].runner.rounds[r].shouldPass){ %>
                                                            <span class="badge rounded-pill  bg-primary">PASS</span>
                                                            <%} else {%> <span
                                                                    class="badge rounded-pill  bg-primary">FAIL</span>
                                                                <% } %>
                                                                    <% } else { %>Test Passed: <span
                                                                            class="badge rounded-pill  bg-secondary">✋ PAUSED</span>
                                                                        <% } %>
                                                                            <%= scenarios[i].steps.steps[x].runner.rounds[r].when
                                                                                %>

                                                                            <% if(scenarios[i].steps.steps[x].runner.rounds[r].assert) { %>
                                                                                <br>
                                                                                <span
                                                                            class="badge rounded-pill  bg-danger"><%= scenarios[i].steps.steps[x].runner.rounds[r].assert %></span>
                                                                                
                                                                                <% } %>

                                    </a>

                                    <% } %>
                                    <% } else { %>
                                        <a href="#" class="list-group-item list-group-item-action">
                                            <span class="badge bg-secondary">FAILED TO RUN</span>
                                        </a>
                                        <% } %>

                            </div>
                            <% if (scenarios[i].steps.steps[x].runner) { %>
                            <% for( var r=0; r < scenarios[i].steps.steps[x].runner.rounds.length; r++ ) { %>
                                <div class="offcanvas offcanvas-start" tabindex="-1"
                                    id="<%=scenarios[i].steps.steps[x].runner.rounds[r].id %>"
                                    aria-labelledby="<%=scenarios[i].steps.steps[x].runner.rounds[r].id %>Label"
                                    style="width: 50%;">
                                    <div class="offcanvas-header">
                                        <h5 class="offcanvas-title"
                                            id="<%=scenarios[i].steps.steps[x].runner.rounds[r].id %>Label">
                                            Test Passed:<%if(scenarios[i].steps.steps[x].runner.rounds[r].passed) {%>
                                                ✅<% }else{%>❌<%}%> | SHOULD <%
                                                            if(scenarios[i].steps.steps[x].runner.rounds[r].pass){ %>
                                                            <span class="badge rounded-pill  bg-primary">PASS</span>
                                                            <%} else {%> <span
                                                                    class="badge rounded-pill  bg-primary">FAIL</span>
                                                                <% } %>
                                                                    <%= scenarios[i].steps.steps[x].runner.rounds[r].when
                                                                        %>
                                        </h5>
                                        <button type="button" class="btn-close" data-bs-dismiss="offcanvas"
                                            aria-label="Close"></button>
                                    </div>
                                    <div class="offcanvas-body">

                                        <div class="card text-bg-light mb-3">
                                            <div class="card-body">

                                                request
                                                <pre class="px-2 py-2 "
                                                    style="background-color: black; color: aliceblue; border-radius: 8px;"><%- JSON.stringify(scenarios[i].steps.steps[x].runner.rounds[r].req, undefined, 2) %></pre>

                                                response <% if(scenarios[i].steps.steps[x].runner.rounds[r].passed){
                                                    %> <span class="badge bg-success">Passed</span>
                                                    <%} else {%> <span class="badge bg-danger">Failed</span>
                                                        <% } %>
                                                            <pre class="px-2 py-2 "
                                                                style="background-color: darkgreen; color: aliceblue; border-radius: 8px;"><%- JSON.stringify(scenarios[i].steps.steps[x].runner.rounds[r].res, undefined, 2) %></pre>

                                            </div>
                                        </div>

                                    </div>
                                </div>

                                <% } %>
                                <% }  else {%>
                                    Failed To RUN 
                                    <% } %>




                                    <div class="card-footer text-body-secondary">
                                        ...
                                    </div>
                        </div>


                        <% } %>
                </div>
            </div>
            </div>

            <% } %>
    </main>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"></script>
    <script>
        /* global bootstrap: false */
        (() => {
            'use strict'
            const tooltipTriggerList = Array.from(document.querySelectorAll('[data-bs-toggle="tooltip"]'))
            tooltipTriggerList.forEach(tooltipTriggerEl => {
                new bootstrap.Tooltip(tooltipTriggerEl)
            })
        })()

    </script>

</body>

</html>